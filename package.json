{
  "name": "@lifaon/observables",
  "version": "1.2.0",
  "description": "Better observables for everyone",
  "main": "bundle/public.umd.esnext.min.js",
  "module": "index.js",
  "jsnext:main": "bundle/public.umd.esnext.min.js",
  "scripts": {
    "start": "npm run clean && gulp watch",
    "clean": "rimraf ./dist",
    "build.prod": "npm run clean && gulp build.prod && npm run minify",
    "babelify": "babel ./dist/bundle/public.umd.esnext.js --out-file ./dist/bundle/public.umd.es5.js --presets=@babel/preset-env",
    "minify": "npm run min.esnext && npm run min.core",
    "min.esnext": "uglifyjs ./dist/bundle/public.umd.esnext.js -o ./dist/bundle/public.umd.esnext.min.js -c inline=false -m",
    "min.core": "uglifyjs ./dist/bundle/public.core.umd.esnext.js -o ./dist/bundle/public.core.umd.esnext.min.js -c inline=false -m",
    "publish.prod": "cd ./dist && npm publish --access=public",
    "serve": "http-server ./dist --ssl -p 1234",
    "serve-cors": "no-cors-proxy -p 4321 -t https://localhost:1234",
    "toc": "markdown-toc README.md"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lifaon74/observables"
  },
  "author": "valentin",
  "license": "MIT",
  "access": "public",
  "unpkg": "bundle/public.umd.esnext.min.js",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@types/node": "latest",
    "gulp": "^4.0.2",
    "gulp-cached": "^1.1.1",
    "gulp-debug": "^4.0.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-progeny": "^0.4.1",
    "gulp-rename": "^1.4.0",
    "gulp-rollup": "^2.16.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-typescript": "^5.0.1",
    "gulp-util": "latest",
    "http-server": "^0.11.1",
    "markdown-toc": "^1.2.0",
    "nodemon": "^1.19.0",
    "rimraf": "^2.6.3",
    "rollup-plugin-node-resolve": "^4.2.4",
    "rxjs": "^6.5.2",
    "typescript": "^3.5.0-dev.20190529",
    "typescript-tuple": "^2.2.1",
    "uglify-es": "^3.3.9",
    "vinyl-source-stream": "latest"
  },
  "dependencies": {}
}
